<div class="login">
	<div class="container">
		<div class="form">
      <br>
      <br>
      <h1>Log in</h1>
      <%= form_for(:session, url: login_path) do |f| %> 
      <%= f.email_field :user_email, :placeholder => "Email" %> 
      <%= f.password_field :password, :placeholder => "Password" %> 
      <%= f.submit "Log in", class: "btn-submit" %>
      <% end %>
    </div>
	</div>
</div>