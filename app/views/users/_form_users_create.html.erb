<div class="form">
	<%=form_for @user do |f| %>
		<%= f.text_field :user_name, :placeholder => "Name" %>
		<br>
		<%= f.text_field :user_last_name, :placeholder => "Last Name" %>
		<br>
		<%= f.email_field :user_email, :placeholder => "Email" %>
		<br>
		Add a password
		<%= f.password_field :password, :placeholder => "Password" %>
		<br>
		<%= f.submit 'Create Account', class: "btn-submit" %>
	<% end %>
</div>